        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:studio": "prisma studio",
        "clean": "rimraf dist .turbo node_modules"
    },
    "dependencies": {
        "@prisma/client": "^5.22.0"
    },
    "devDependencies": {
        "prisma": "^5.22.0",
        "tsup": "^8.3.0",
        "typescript": "^5.3.0"
    }
}